                            <li>
                                <a class="tooltip-tip ajax-load" href="#/notifications" title="Track">
                                    <span>Notifications</span>
                                </a>
                            </li>
                        </ul>
                        <ul class="topnav menu-left-nest">
                            <li>
                                <a class="tooltip-tip ajax-load" href="#/routemap" title="Track">
                                    <span>Route Map</span>
                                </a>
                            </li>
                        </ul>
                        <ul class="topnav menu-left-nest">
                            <li>
                                <a class="tooltip-tip ajax-load" href="#/pinbyzip" title="Track">
                                    <span>Search by Zipcode</span>
                                </a>
                            </li>
                        </ul>

                        @{if (ViewContext.Controller.HasPermission("TRK"))
                            {



                                <ul class="topnav menu-left-nest">

                                    <li>
                                        <a class="tooltip-tip ajax-load" href="/Maps/Test" title="Track">

                                            <span>Track</span>
                                        </a>

                                    </li>
                                </ul>

                            }}



                        <ul class="topnav menu-left-nest">




                            @{if (ViewContext.Controller.HasPermission("CRTUSR"))
                                {



                                    <li>
                                        <a class="tooltip-tip ajax-load" href="/Maps/CreateUser" title="New User">

                                            <span>Add New User</span>
                                        </a>
                                    </li>

                                }}


                            @{if (ViewContext.Controller.HasPermission("USRINDX"))
                                {



                                    <li>
                                        <a class="tooltip-tip ajax-load" href="/AppUser/Index" title="List User">

                                            <span>User Index</span>
                                        </a>
                                    </li>

                                }}


                            @{if (ViewContext.Controller.HasPermission("VIEWROLES"))
                                {



                                    <li>
                                        <a class="tooltip-tip ajax-load" href="/AppUser/Roles" title="List Roles">

                                            <span>View Roles</span>
                                        </a>
                                    </li>

                                }}

                            @{if (ViewContext.Controller.HasPermission("CRTROLES"))
                                {



                                    <li>
                                        <a class="tooltip-tip ajax-load" href="/AppUser/CreateRole" title="Create Roles">

                                            <span>Create  Roles</span>
                                        </a>
                                    </li>

                                }}





                            @{if (ViewContext.Controller.HasPermission("PLCY"))
                                {

                                    <li>
                                        <a class="tooltip-tip ajax-load" href="#" title="Pallet">

                                            <span>Pallet Life Cycle</span>

                                        </a>
                                    </li>




                                }}



                            @{if (ViewContext.Controller.HasPermission("NTFS"))
                                {

                                    <li>
                                        <a class="tooltip-tip ajax-load" href="#" title="Pallet">

                                            <span>Notification</span>

                                        </a>
                                    </li>




                                }}
                        </ul>





                        <ul id="menu-showhide" class="topnav menu-left-nest">






                            @{if (ViewContext.Controller.HasPermission("DVMNG"))
                                {
                                    <li>
                                        <a class="tooltip-tip ajax-load" href="#" title="Map">

                                            <span>Device Manager</span>
                                        </a>
                                    </li>

                                } }


                        </ul>

                    </div>
                </div>
            </div>
        </div>
        <!-- END OF SIDE MENU -->
        @RenderBody()



